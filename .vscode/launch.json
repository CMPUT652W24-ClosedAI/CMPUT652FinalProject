  "version": "0.2.0",
  "configurations": [

    {
      "name": "Experiment 1: ppo_gridnet_large",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/experiments",
      "request": "launch",
      "program": "ppo_gridnet_large.py",
      "console": "integratedTerminal",
      "args": [
        "--total-timesteps", "300000000",
        "--num-bot-envs", "24",
        "--num-selfplay-envs", "0",
        "--partial-obs", "False"
      ]
    },
    {
      "name": "Experiment 2: evaluate sota",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/experiments",
      "request": "launch",
      "program": "ppo_gridnet_eval.py",
      "console": "integratedTerminal",
      "args": [
        "--agent-model-path", "gym-microrts-static-files/agent_sota.pt",
        "--agent2-model-path", "gym-microrts-static-files/agent_sota.pt",
        "--ai", "randomBiasedAI",
        "--play-video",
      ]
    },
    {
      "name": "Train generator",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/generator",
      "request": "launch",
      "program": "training_script.py",
      "console": "integratedTerminal",
      "args": [
        "--num-episodes", "10",
        "--episode-length", "32",
        "--replay-buffer-size", "100",
        "--step-jump", "2",
        "--asym_to_fairness_ratio", "0.8",
        "--wall-reward", "0.1",
        "--visualize_maps",
      ]
    },
    {
      "name": "Train generator testing",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/generator",
      "request": "launch",
      "program": "training_script.py",
      "console": "integratedTerminal",
      "args": [
        "--num-episodes", "1000",
        "--episode-length", "64",
        "--replay-buffer-size", "1000",
        "--step-jump", "4",
        "--asym_to_fairness_ratio", "0.8",
        "--wall-reward", "0.1",
        // "--use-baseline",
        // "--visualize_maps",
      ]
    },
    {
      "name": "Run Maploader",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/generator",
      "request": "launch",
      "program": "run_maploader.py",
      "console": "integratedTerminal",
      "args": [
        "--num-episodes", "10",
        "--episode-length", "32",
        "--replay-buffer-size", "100",
        "--step-jump", "2",
        "--visualize_maps",
      ]
    },
    {
      "name": "Run map generator",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/generator",
      "request": "launch",
      "program": "map_generator.py",
      "console": "integratedTerminal",
      "args": [
        "--input-map-path", "input_maps/defaultMap.xml",
        "--model-path", "models/policy_net_non_episodic_rewards.pt",
        "--num-maps", "100",
        "--episode_length", "64",
      ]
    },
    {
      "name": "Run map generator custom agent",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/generator",
      "request": "launch",
      "program": "map_generator.py",
      "console": "integratedTerminal",
      "args": [
        "--input-map-path", "input_maps/defaultMap.xml",
        "--model-path", "models/output/training_net_eps_1.0_tau_0.005_ratio_0.8_wr_0.1_ep_1000_len_64_baseline_True___1733022366.pt",
        "--num-maps", "1000",
        "--episode_length", "64",
        "--top-k", "5",
      ]
    },
    {
      "name": "Run map generator random agent",
      "type": "debugpy",
      "cwd": "${workspaceFolder}/generator",
      "request": "launch",
      "program": "map_generator.py",
      "console": "integratedTerminal",
      "args": [
        "--input-map-path", "input_maps/defaultMap.xml",
        "--model-path", "models/output/training_net_eps_1.0_tau_0.005_ratio_0.8_wr_0.1_ep_1000_len_64_baseline_True___1733022366.pt",
        "--num-maps", "1000",
        "--episode_length", "64",
        "--use-random"
      ]
    },
    {
      "name": "dont use",
      "args": [
        "--model-path", "models/output/training_net_eps_1.0_tau_0.005_ratio_0.8_wr_0.1_ep_1000_len_64_baseline_True___1733022366.pt",
      ]
    }

  ]
}